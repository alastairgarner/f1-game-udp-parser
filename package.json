{
  "name": "f1-game-udp-parser",
  "version": "0.1.33",
  "description": "Rust built Node compatible udp parser for Codemaster's Formula 1 games",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/race-hub/f1-game-udp-parser.git"
  },
  "bugs": {
    "url": "https://github.com/race-hub/f1-game-udp-parser/issues"
  },
  "homepage": "https://github.com/race-hub/f1-game-udp-parser#readme",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "native/src",
    "native/Cargo.lock",
    "native/Cargo.toml",
    "native/build.rs",
    "native/index.node",
    "lib/"
  ],
  "author": "msansoni <f1-game-udp-parser@michaelsansoni.com>",
  "contributors": [
    {
      "name": "Michael Sansoni",
      "email": "f1-game-upd-parser@michaelsansoni.com",
      "url": "michaelsansoni.com"
    },
    {
      "name": "Jonathan Bursztyn",
      "email": "jonathan@bursztyn.io",
      "url": "bursztyn.io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.3.3",
    "node-pre-gyp": "amilajack/node-pre-gyp#neon-compat"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "check": "gts check",
    "clean": "gts clean",
    "test": "jest --detectOpenHandles",
    "prebuild": "rm -rf ./lib",
    "build": "tsc",
    "postbuild": "mv ./lib/src/* ./lib/ && rm -rf ./lib/src",
    "fix": "gts fix"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/race-hub/f1-game-udp-parser/releases/download/",
    "remote_path": "v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  },
  "devDependencies": {
    "@types/node": "^10.0.3",
    "@types/jest": "^25.1.2",
    "gts": "^1.1.2",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "~3.7.0"
  },
  "keywords": [
    "automotive",
    "grand prix",
    "f12019",
    "f1 2019",
    "formula 1",
    "f1",
    "codemasters",
    "udp",
    "telemetry",
    "racing",
    "simracing",
    "gaming",
    "formula",
    "ps4",
    "xbox",
    "xbox one",
    "playstation",
    "playstation 4",
    "esports",
    "e sports",
    "e-sports",
    "videogame"
  ]
}
